nodes:
  - name: SupplierDimension
    type: scd
    config:
      subtype: spark
      conf: conf/f_scd_supplierdimension.json
      target: hive
      explain_only: 'false'
      source_table: test.supplier
      change_tolerance: '100'
      target_table: test.dim_supplier
    dependsOn:
      - Transfer_Supplier
  - name: Transfer_Supplier
    type: transfer
    config:
      subtype: spark
      conf: conf/f_scd_transfer_supplier.json
      source: PGLab
      target: hive
      explain_only: 'false'
      empty_source_check: 'false'
      target_table: test.supplier
      source_table: public.source_supplier
    dependsOn:
      - Truncate_table
  - name: TruncateDimOnVertica
    type: transform
    config:
      target: vertica
      sql_file: sql/f_scd_truncatedimonvertica.sql
      explain_only: false
    dependsOn:
      - SupplierDimension
  - name: LoadIntoVertica
    type: transfer
    config:
      subtype: spark
      conf: conf/f_scd_loadintovertica.json
      source: hive
      target: vertica
      source_table: test.dim_supplier
      explain_only: false
      empty_source_check: true
      target_table: henkan_demo.dim_supplier
    dependsOn:
      - TruncateDimOnVertica
  - name: Refresh_Parameters
    type: subflow
    config:
      flow.name: Refresh_Parameters.subflow
    dependsOn: []
  - name: ScdIndb
    type: scd
    config:
      subtype: indb
      conf: conf/f_simplescd_scdindb.json
      target: vertica
      explain_only: false
      change_tolerance: '100'
      target_table: henkan_demo.dim_supplier_indb
      sql_file: sql/f_simplescd_scdindb.sql
    dependsOn:
      - TransferSupplierToVertica
  - name: TransferSupplierToVertica
    type: transfer
    config:
      subtype: spark
      conf: conf/f_simplescd_transfersuppliertovertica.json
      source: PGLab
      target: vertica
      source_table: public.supplier
      explain_only: false
      empty_source_check: true
      target_table: henkan_demo.supplier
    dependsOn:
      - truncate_supplier
  - name: Truncate_table
    type: transform
    config:
      target: v360hive
      sql_file: sql/f_simplescd_truncate_table.sql
      explain_only: false
    dependsOn:
      - Refresh_Parameters
  - name: truncate_supplier
    type: transform
    config:
      target: vertica
      sql_file: sql/f_simplescd_truncate_supplier.sql
      explain_only: false
    dependsOn:
      - Refresh_Parameters
  - name: truncate_henkan_demo.customer
    type: transform
    config:
      target: vertica
      sql_file: Flows/sql/f_simplescd_truncate_henkan_demo.customer.sql
      explain_only: false
    dependsOn: []
  - name: transfer_customer_sqlserver_to_vertica
    type: transfer
    config:
      subtype: spark
      conf: Flows/conf/f_simplescd_transfer_customer_sqlserver_to_vertica.json
      source: Sqlserver
      target: vertica
      explain_only: false
      empty_source_check: true
      target_table: henkan_demo.customer
      source_table: TESTDB.dbo.customer
    dependsOn:
      - truncate_henkan_demo.customer
  - name: dim_customer
    type: scd
    config:
      subtype: indb
      conf: Flows/conf/f_simplescd_dim_customer.json
      target: vertica
      explain_only: false
      source_table: henkan_demo.customer
      change_tolerance: 10
      target_table: henkan_demo.dim_customer
    dependsOn:
      - transfer_customer_sqlserver_to_vertica
  - name: transfer_customer_sqlserver_to_hive
    type: transfer
    config:
      subtype: spark
      conf: Flows/conf/f_simplescd_transfer_customer_sqlserver_to_hive.json
      source: Sqlserver
      target: hive
      source_table: testdb.dbo.customer
      explain_only: false
      empty_source_check: true
      target_table: henkan_demo.customer
    dependsOn: []
  - name: scd_customer
    type: scd
    config:
      subtype: spark
      conf: Flows/conf/f_simplescd_scd_customer.json
      target: hive
      explain_only: false
      source_table: henkan_demo.customer
      change_tolerance: 10
      target_table: henkan_demo.dim_customer
    dependsOn:
      - transfer_customer_sqlserver_to_hive
  - name: transfer_dim_customer_hive_to_vertica
    type: transfer
    config:
      subtype: spark
      conf: Flows/conf/f_simplescd_transfer_dim_customer_hive_to_vertica.json
      source: hive
      target: vertica
      source_table: henkan_demo.dim_customer
      explain_only: false
      empty_source_check: true
      target_table: henkan_demo.dim_customer
    dependsOn:
      - truncate_dim_customer
  - name: truncate_dim_customer
    type: transform
    config:
      target: vertica
      sql_file: Flows/sql/f_simplescd_truncate_dim_customer.sql
      explain_only: false
    dependsOn:
      - scd_customer
config: {}
