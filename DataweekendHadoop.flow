nodes:
  - name: transfer_orders_pg_to_hive
    type: transfer
    config:
      subtype: spark
      conf: conf/f_dataweekendhadoop_transfer_orders_pg_to_hive.json
      source: PGLab
      target: v360hive
      explain_only: false
      empty_source_check: 'false'
      source_table: public.orders
      target_table: demo.orders
    dependsOn: []
  - name: transfer_customer_pg_to_hive
    type: transfer
    config:
      subtype: spark
      conf: conf/f_dataweekendhadoop_transfer_customer_pg_to_hive.json
      source: PGLab
      target: v360hive
      source_table: public.customer
      explain_only: false
      empty_source_check: true
      target_table: demo.customer
    dependsOn:
      - transfer_orders_pg_to_hive
  - name: dim_customer
    type: scd
    config:
      subtype: spark
      conf: conf/f_dataweekendhadoop_dim_customer.json
      target: hive
      explain_only: false
      source_table: demo.customer
      change_tolerance: 10
      target_table: demo.dim_customer
    dependsOn:
      - transfer_customer_pg_to_hive
config: {}
