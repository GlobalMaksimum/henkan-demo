nodes:
  - name: T_TRANSFER
    type: transfer
    config:
      subtype: spark
      conf: conf/f_t_main_t_transfer.json
      source: PGLab
      target: vertica
      source_table: T_TRANSFERTable
      sql_file: sql/f_t_main_t_transfer.sql
      explain_only: false
      empty_source_check: 'false'
    dependsOn:
      - T_PARAMETER
  - name: T_PARAMETER
    type: parameter
    config:
      param_name: Parameter name
      target: v360hive
      sql_file: sql/f_t_main_t_parameter.sql
    dependsOn: []
  - name: T_TRANSFORM
    type: transform
    config:
      target: T_TRANSFORMTarget
      sql_file: sql/f_t_main_t_transform.sql
      explain_only: false
    dependsOn:
      - T_TRANSFER
config: {}
