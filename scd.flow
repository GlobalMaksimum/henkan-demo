nodes:
  - name: SupplierDimension
    type: scd
    config:
      subtype: spark
      conf: conf/f_scd_supplierdimension.json
      target: hive
      explain_only: 'false'
      source_table: default.supplier
      change_tolerance: '100'
      target_table: default.dim_supplier
    dependsOn:
      - TransferSupplier
  - name: TransferSupplier
    type: transfer
    config:
      subtype: spark
      conf: conf/f_scd_transfersupplier.json
      source: PGLab
      target: hive
      explain_only: false
      empty_source_check: true
      target_table: default.supplier
      source_table: supplier
    dependsOn: []
  - name: TruncateDimOnVertica
    type: transform
    config:
      target: vertica
      sql_file: sql/f_scd_truncatedimonvertica.sql
      explain_only: false
    dependsOn:
      - SupplierDimension
  - name: LoadIntoVertica
    type: transfer
    config:
      subtype: spark
      conf: conf/f_scd_loadintovertica.json
      source: hive
      target: vertica
      source_table: default.dim_supplier
      explain_only: false
      empty_source_check: true
      target_table: henkan_demo.dim_supplier
    dependsOn:
      - TruncateDimOnVertica
config: {}
