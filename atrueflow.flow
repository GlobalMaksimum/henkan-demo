nodes:
  - name: PGToVertica
    type: transfer
    config:
      subtype: spark
      conf: conf/f_atrueflow_pgtovertica.json
      source: PGLab
      target: veon10
      explain_only: false
      empty_source_check: true
      target_table: public.lineitem
      source_table: public.lineitem
    dependsOn:
      - TruncateTableFirst
  - name: AggOnVertica
    type: transform
    config:
      target: veon10
      sql_file: sql/f_atrueflow_aggonvertica.sql
      explain_only: false
    dependsOn:
      - TruncateVertica
  - name: TruncateVertica
    type: transform
    config:
      target: veon10
      sql_file: sql/f_atrueflow_truncatevertica.sql
      explain_only: false
    dependsOn:
      - PGToVertica
  - name: TruncateTableFirst
    type: transform
    config:
      target: veon10
      sql_file: sql/f_atrueflow_truncatetablefirst.sql
      explain_only: false
    dependsOn: []
config: {}
